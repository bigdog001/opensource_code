<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ok.bean.IndexImg" table="indeximg">

        <id name="imgid" type="java.lang.Integer">
            <column name="imgid" />
            <generator class="native" />
        </id>

        <property name="oedds" />
        <property name="img" />
        <property name="thumb" />
        <property name="city" />
        <property name="star" />
        <property name="truename" />
        <property name="target_url" />
        <property name="creattime" />

    </class>

</hibernate-mapping>
