<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ok.bean.VisitFooter" table="visitfooter">

        <id name="footerid" type="java.lang.Integer">
            <column name="footerid" />
            <generator class="native" />
        </id>

        <property name="my_user_id" />
        <property name="friends_user_id" />
        <property name="visit_time" />
    </class>

</hibernate-mapping>
